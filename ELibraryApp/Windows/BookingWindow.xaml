<Window x:Class="ELibraryApp.Views.BookingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ELibraryApp.Views"
        mc:Ignorable="d"
        Title="Бронирование" Height="282" Width="389" Loaded="Window_Loaded">
    <Grid>
        <Label Content="Дата начала бронирования" HorizontalAlignment="Left" Margin="25,31,0,0" VerticalAlignment="Top" Height="40" Width="200"/>
        <Label Content="Дата окончания бронирования" HorizontalAlignment="Left" Margin="25,95,0,0" VerticalAlignment="Top" Height="40" Width="200"/>
        <DatePicker Name="StartDatePicker" HorizontalAlignment="Left" Margin="230,32,0,0" VerticalAlignment="Top" Height="39" Width="125"/>
        <DatePicker Name="EndDatePicker" HorizontalAlignment="Left" Margin="230,95,0,0" VerticalAlignment="Top" Height="40" Width="125"/>
        <Button Content="Подать заявку" HorizontalAlignment="Left" Margin="95,165,0,0" VerticalAlignment="Top" Width="200" Height="50" Click="SaveButton_Click"/>

    </Grid>
</Window>
